                                                                                <RadzenDataGridColumn TItem="ConsultationPartyListVM" Property="Email" Title="@translationState.Translate("Email")" />
                                                                                <RadzenDataGridColumn TItem="ConsultationPartyListVM" Property="PhoneNumber" Title="@translationState.Translate("Phone_Number")" />
                                                                                <RadzenDataGridColumn TItem="ConsultationPartyListVM" Property="Fax" Title="@translationState.Translate("Fax")" />
                                                                                <RadzenDataGridColumn TItem="ConsultationPartyListVM" Property="POBox" Title="@translationState.Translate("PO_Box")" />
                                                                                <RadzenDataGridColumn TItem="ConsultationPartyListVM" Property="POCode" Title="@translationState.Translate("PO_Code")" />

                                                                            </Columns>
                                                                        </RadzenDataGrid>
                                                                    </div>
                                                                </div>
