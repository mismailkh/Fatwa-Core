                                            @if (SectorTypeIdCheck == (int)OperatingSectorTypeEnum.Contracts && !string.IsNullOrWhiteSpace(consultationRequestVM.Introduction))
                                            {
                                                <RadzenAccordion>
                                                    <Items>
                                                        <RadzenAccordionItem Text="@translationState.Translate("Introduction")">
                                                            <div class="col-md-12">
                                                                <div class="">
                                                                    <RadzenHtmlEditor Placeholder="@translationState.Translate("Consultation_Introduction")" @bind-Value=@(@consultationRequestVM.Introduction) Disabled />

                                                                 </div>

                                                             </div>


                                                         </RadzenAccordionItem>
                                                     </Items>
                                                 </RadzenAccordion>
                                            }
                                            @if (SectorTypeIdCheck == (int)OperatingSectorTypeEnum.Contracts && consultationArticleByConsultationIdListVM.Count() != 0)
                                            {
                                                <RadzenAccordion>
                                                    <Items>
                                                        <RadzenAccordionItem Text="@translationState.Translate("Article")">
                                                            <div class="row pb-2">
                                                                <div class="col-12 datagrid-rz-label">
                                                                    <div class="tableContent ">

                                                                        <div class="row no-gutters">

                                                                            <RadzenDataGrid @ref="consultationArticleGrid"
                                                                                            AllowPaging="true"
                                                                                            PageSize="5"
                                                                                            Data="@consultationArticleByConsultationIdListVM"
                                                                                            TItem="ConsultationArticleByConsultationIdListVM">
                                                                                <Columns>
                                                                                    <RadzenDataGridColumn TItem="ConsultationArticleByConsultationIdListVM" Title="@translationState.Translate("Action")" Filterable="false" Sortable="false" Width="90px" TextAlign="TextAlign.Center">

                                                                                    </RadzenDataGridColumn>
                                                                                    <RadzenDataGridColumn TItem="ConsultationArticleByConsultationIdListVM" Property="SectionName" Title="@translationState.Translate("Section_Name")" />


                                                                                    <RadzenDataGridColumn TItem="ConsultationArticleByConsultationIdListVM" Property="ArticleTitle" Title="@translationState.Translate("Article_Title")" />
                                                                                    <RadzenDataGridColumn TItem="ConsultationArticleByConsultationIdListVM" Property=@(Thread.CurrentThread.CurrentUICulture.Name == "en-US" ? "StatusEn" : "StatusAr") Title="@translationState.Translate("Status")" />
                                                                                </Columns>
                                                                            </RadzenDataGrid>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </RadzenAccordionItem>
                                                    </Items>
                                                </RadzenAccordion>
                                            }
                                            <RadzenAccordionItem Text="@translationState.Translate("List_Documents")">
                                                <FileUpload ReferenceGuid="Guid.Parse(ConsultationRequestId)"
                                                            IsViewOnly="true"
                                                            IsCaseRequest="true"
                                                            SubModuleId="(int)SubModuleEnum.ConsultationRequest"
                                                            ModuleId="(int)WorkflowModuleEnum.COMSConsultationManagement"
                                                            ShowCorrespondenceButton="true" />
                                            </RadzenAccordionItem>
                                        }
                                    </Items>
                                </RadzenAccordion>
                            </RadzenTabsItem>
                            <RadzenTabsItem Text="@translationState.Translate("Communication_Key")">
                                <div class="row pb-1">
                                    <div class="col-12 datagrid-rz-label">
                                        <div class="ow tableContent table-responsive no-gutters pb-3 ">
                                            <RadzenDataGrid @ref="CommunicationGrid"
                                                            AllowPaging="true"
                                                            PageSize="@systemSettingState.Grid_Pagination"
                                                            ColumnWidth="250px"
                                                            Data="@communicationListVm"
                                                            TItem="CommunicationListVM"
                                                            EmptyText="@translationState.Translate("No_record_found")"
                                                            Width="100%"
                                                            AllowFiltering="false"
                                                            FilterMode="Radzen.FilterMode.Advanced"
                                                            AllowSorting="true"
                                                            PagerHorizontalAlign="Radzen.HorizontalAlign.Center">
                                                <Columns>
                                                    <RadzenDataGridColumn TItem="CommunicationListVM" Title="@translationState.Translate("Action")"
                                                                          Filterable="false"
                                                                          Sortable="false"
                                                                          Width="100px"
                                                                          TextAlign="TextAlign.Center">
                                                        <Template Context="rowItem">
                                                            @if (rowItem.CommunicationTypeId < (int)CommunicationTypeEnum.ContractRequest)
                                                            {
                                                                @if (rowItem.IsRead != true)
                                                                {
                                                                    <RadzenButton ButtonStyle="ButtonStyle.Secondary"
                                                                                  Icon="visibility"
                                                                                  Size="ButtonSize.Small"
                                                                                  Click="@((args) => ViewResponse(rowItem))"
                                                                    @onclick:stopPropagation="true" />

                                                                }
                                                                else
                                                                {
                                                                    <RadzenButton Style="background-color: #06be3d !important;" ButtonStyle="ButtonStyle.Secondary"
                                                                                  Icon="visibility"
                                                                                  Size="ButtonSize.Small"
                                                                                  Click="@((args) => ViewResponse(rowItem))"
                                                                    @onclick:stopPropagation="true" />
                                                                }
                                                            }
                                                            @if (rowItem.CommunicationTypeId == (int)CommunicationTypeEnum.RequestForMeeting && rowItem.CorrespondenceTypeId == (int)CommunicationCorrespondenceTypeEnum.Inbox)
                                                            {
                                                                <RadzenButton ButtonStyle="ButtonStyle.Secondary"
                                                                              Class="editIcon"
                                                                              Icon="mode"
                                                                              Title="@translationState.Translate("Schedule_Meeting")"
                                                                              Size="ButtonSize.Small"
                                                                              Click="@((args) => AddMeeting(rowItem))"
                                                                @onclick:stopPropagation="true" />
                                                            }

                                                        </Template>
                                                    </RadzenDataGridColumn>
                                                    <RadzenDataGridColumn TItem="CommunicationListVM" Property="InboxNumber" Title="@translationState.Translate("InboxNo")" />
                                                    <RadzenDataGridColumn TItem="CommunicationListVM" Property="InboxDate" Title="@translationState.Translate("InboxDate")" FormatString="{0:dd/MM/yyyy hh:mm tt}" />
                                                    <RadzenDataGridColumn TItem="CommunicationListVM" Property="OutboxNumber" Title="@translationState.Translate("OutboxNo")" />
                                                    <RadzenDataGridColumn TItem="CommunicationListVM" Property="OutboxDate" Title="@translationState.Translate("OutboxDate")" FormatString="{0:dd/MM/yyyy hh:mm tt}" />
                                                    <RadzenDataGridColumn TItem="CommunicationListVM" Property=@(Thread.CurrentThread.CurrentUICulture.Name == "en-US" ? "CorrespondenceTypeEn" : "CorrespondenceTypeAr") Title="@translationState.Translate("Correspondence_Type")" />
                                                    <RadzenDataGridColumn TItem="CommunicationListVM" Property=@(Thread.CurrentThread.CurrentUICulture.Name == "en-US" ? "Activity_En" : "Activity_Ar") Title="@translationState.Translate("Activity_Title")" />
                                                    <RadzenDataGridColumn TItem="CommunicationListVM" Property="CreatedBy" Title="@translationState.Translate("Initiated_By")" />
                                                    <RadzenDataGridColumn TItem="CommunicationListVM" Property="CreatedDate" Title="@translationState.Translate("Date")" FormatString="{0:dd/MM/yyyy hh:mm tt}" />
                                                </Columns>
                                            </RadzenDataGrid>
                                        </div>
                                    </div>
                                </div>
                            </RadzenTabsItem>
                            @* <CommunicationGridComponenent ConsultationRequestId="@ConsultationRequestId" communicationListVm="@communicationListVm"/>*@
                            <RadzenTabsItem Text="@translationState.Translate("History_Details")">
                                <RadzenAccordion>
                                    <Items>
                                        <RadzenAccordionItem Text="@translationState.Translate("Status_History")" Selected="true">
                                            <div class="row pb-1">
                                                <div class="col-12 datagrid-rz-label">
                                                    <div class="tableContent table-responsive no-gutters pb-3 ">
                                                        <RadzenDataGrid @ref="HistoryGrid"
                                                                        AllowPaging="true"
                                                                        PageSize="@systemSettingState.Grid_Pagination"
                                                                        Data="@cmsConsultationRequestHistory"
                                                                        TItem="ComsConsultationRequestHistoryVM"
                                                                        EmptyText="@translationState.Translate("No_record_found")"
                                                                        Width="100%"
                                                                        AllowFiltering="false"
                                                                        FilterMode="Radzen.FilterMode.Advanced"
                                                                        AllowSorting="true"
                                                                        PagerHorizontalAlign="Radzen.HorizontalAlign.Center">
                                                            <Columns>
                                                                <RadzenDataGridColumn TItem="ComsConsultationRequestHistoryVM" Property=@(Thread.CurrentThread.CurrentUICulture.Name == "en-US" ? "EventEn" : "EventAr") Title="@translationState.Translate("Event")">
                                                                </RadzenDataGridColumn>
                                                                <RadzenDataGridColumn TItem="ComsConsultationRequestHistoryVM" Property="CreatedDate" FormatString="{0:dd/MM/yyyy}" Title="@translationState.Translate("Date")" />
                                                                <RadzenDataGridColumn TItem="ComsConsultationRequestHistoryVM" Property="Remarks" Title="@translationState.Translate("Remarks")">
                                                                    <Template Context="remarks">
                                                                        @if (remarks.Remarks != null)
                                                                        {
                                                                            @if (remarks.Remarks.Length > 40)
                                                                            {
                                                                                @remarks.Remarks.Substring(0, 40) <a href="#" @onclick="@(() => OnDescriptionFullRemarksDetail(remarks.Remarks))" @onclick:preventDefault>@translationState.Translate("Role_Description_3_dot")</a>
                                                                            }
                                                                            else
                                                                            {
                                                                                @remarks.Remarks
                                                                            }
                                                                        }
                                                                    </Template>
                                                                </RadzenDataGridColumn>

                                                            </Columns>
                                                        </RadzenDataGrid>
                                                    </div>
                                                </div>
                                            </div>
                                        </RadzenAccordionItem>
                                    </Items>
                                </RadzenAccordion>
                                <RadzenAccordion>
                                    <Items>
                                        <RadzenAccordionItem Text="@translationState.Translate("Transfer_History")" Selected="true">
                                            <div class="row pb-2">
                                                <div class="col-12 datagrid-rz-label">
                                                    <div class="tableContent ">
                                                        <RadzenDataGrid @ref="transferHistoryGrid"
                                                                        AllowPaging="true"
                                                                        PageSize="@systemSettingState.Grid_Pagination"
                                                                        Data="@cmsCaseRequestTransferHistory"
                                                                        TItem="CmsTransferHistoryVM"
                                                                        EmptyText="@translationState.Translate("No_record_found")"
                                                                        Width="100%"
                                                                        AllowFiltering="false"
                                                                        FilterMode="Radzen.FilterMode.Advanced"
                                                                        AllowSorting="false"
                                                                        PagerHorizontalAlign="Radzen.HorizontalAlign.Center">
                                                            <Columns>
                                                                <RadzenDataGridColumn Width="60px" TItem="CmsTransferHistoryVM" Property=@(Thread.CurrentThread.CurrentUICulture.Name == "en-US" ? "SectorFromEn" : "SectorFromAr") Title="@translationState.Translate("Sector_From")" />
                                                                <RadzenDataGridColumn Width="60px" TItem="CmsTransferHistoryVM" Property=@(Thread.CurrentThread.CurrentUICulture.Name == "en-US" ? "SectorToEn" : "SectorToAr") Title="@translationState.Translate("Sector_To")" />
                                                                <RadzenDataGridColumn Width="60px" TItem="CmsTransferHistoryVM" Property="Reason" Title="@translationState.Translate("Reason")">
                                                                    <Template Context="reason">
                                                                        @if (reason.Reason != null)
                                                                        {
                                                                            @if (reason.Reason.Length > 30)
                                                                            {
                                                                                @reason.Reason.Substring(0, 30) <a href="#" @onclick="@(() => OnDescriptionFullReasonDetail(reason.Reason))" @onclick:preventDefault>@translationState.Translate("Role_Description_3_dot")</a>
                                                                            }
                                                                            else
                                                                            {
                                                                                @reason.Reason
                                                                            }
                                                                        }
                                                                    </Template>
                                                                </RadzenDataGridColumn>
                                                                <RadzenDataGridColumn Width="60px" TItem="CmsTransferHistoryVM" Property=@(Thread.CurrentThread.CurrentUICulture.Name == "en-US" ? "UserName_En" : "UserName_Ar") Title="@translationState.Translate("Created_By")" />
                                                                <RadzenDataGridColumn Width="60px" TItem="CmsTransferHistoryVM" Property="CreatedDate" FormatString="{0:dd/MM/yyyy}" Title="@translationState.Translate("Date")" />
                                                                <RadzenDataGridColumn Width="60px" TItem="CmsTransferHistoryVM" Property=@(Thread.CurrentThread.CurrentUICulture.Name == "en-US" ? "StatusEn" : "StatusAr") Title="@translationState.Translate("Status")" />
                                                            </Columns>
                                                        </RadzenDataGrid>
                                                    </div>
                                                </div>
                                            </div>
                                        </RadzenAccordionItem>
                                    </Items>
                                </RadzenAccordion>
                            </RadzenTabsItem>
                            @if (loginState.ClaimList.Any(x => x.Value == "Permissions.Submenu.Timelog.TimelogList"))
                            {
                                <RadzenTabsItem Text="@translationState.Translate("Time_Log")">
                                    <TimeTrackingDataGrid ReferenceId="Guid.Parse(ConsultationRequestId)"></TimeTrackingDataGrid>
                                </RadzenTabsItem>
                            }
                        </Tabs>
                    </RadzenTabs>
                </div>
            </div>
            @if (!managerTaskReminderData.Where(x => x.ManagerId == loginState.UserDetail.UserId).Any())
            {
                @if (consultationRequestVM.ConsultationRequestId != Guid.Empty && taskDetailVM?.TaskStatusId != (int)TaskStatusEnum.Done && consultationRequestVM.RequestStatusId != (int)CaseRequestStatusEnum.Draft)
                {

                    <div class="buttoncontainer">
                        <input type="checkbox" id="toggle" />
                        <label class="floatingbutton" for="toggle"></label>
                        <nav class="nav">
                            <ul>
                                @if (!(loginState.UserRoles.Any(x => SystemRoles.ComsLawyer.Contains(x.RoleId))))
                                {
                                    @if (loginState.ClaimList.Where(x => x.Value == "Permissions.COMS.ConsultationFile.Transfer").Any())
                                    {
                                        <TelerikButton Class="cancleBtn"
                                                       ButtonType="Telerik.Blazor.ButtonType.Button"
                                                       ThemeColor="light"
                                                       Enabled="@(consultationRequestVM.RequestStatusId < (int)CaseRequestStatusEnum.RegisteredInMOJ)"
                                                       OnClick="@Transfer"> @translationState.Translate("Transfer") </TelerikButton>
                                    }
                                }
                                @if (!(loginState.UserRoles.Any(x => SystemRoles.ComsLawyer.Contains(x.RoleId))))
                                {
                                    @if (loginState.ClaimList.Where(x => x.Value == "Permissions.COMS.ConsultationFile.AssignToLawyer").Any() && consultationRequestVM.RequestStatusId != (int)CaseRequestStatusEnum.WithdrawRequested)
                                    {

                                        <TelerikButton Class="cancleBtn"
                                                       ButtonType="Telerik.Blazor.ButtonType.Button"
                                                       ThemeColor="light"
                                                       Enabled="@(consultationRequestVM.RequestStatusId < (int)CaseRequestStatusEnum.ConvertedToFile)"
                                                       OnClick="@AssignToLawyer">@translationState.Translate("Assign_To_Lawyer")</TelerikButton>
                                    }
                                }

                            </ul>
                        </nav>
                    </div>
                }
            }
        </ChildContent>
    </RadzenContent>
}
else
{
    <div class="demo-alert demo-alert-error" role="alert">@translationState.Translate("Unauthorized")</div>
}
@code
{
    async Task OnDescriptionFullRemarksDetail(string args)
    {
        await dialogService.OpenAsync(@translationState.Translate("Remarks"), ds =>
    @<div>
        <p class="mb-4" style="min-height:200px;min-width:550px;line-height:2.5">@args</p>
    </div>);
    }
    async Task OnDescriptionFullReasonDetail(string args)
    {
        await dialogService.OpenAsync(@translationState.Translate("Reason"), ds =>
    @<div>
        <p class="mb-4" style="min-height:200px;min-width:550px;line-height:2.5">@args</p>
    </div>);
    }
}
