                                                                                <RadzenDataGridColumn TItem="ConsultationPartyListVM" Property=@(Thread.CurrentThread.CurrentUICulture.Name == "en-US" ? "PartyTypeEn" : "PartyTypeAr") Title="@translationState.Translate("Party_Types")" />
