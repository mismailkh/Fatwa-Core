                                        Data="@ChamberNumbers"
                                        AllowFiltering="true"
                                        FilterCaseSensitivity="FilterCaseSensitivity.CaseInsensitive"
                                        AllowClear="true"
                                        TextProperty="Number"
                                        Name="ChamberNumber"
                                        ValueProperty="Id"
                        @bind-Value="advanceSearch.ChamberNumberId"
                                        Placeholder="@translationState.Translate("Select")"
                                        Style="display: block; width: 100%">
                        </RadzenDropDown>
                    </div>
                </div>
