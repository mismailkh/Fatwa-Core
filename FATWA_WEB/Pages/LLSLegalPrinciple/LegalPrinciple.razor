                    <div class="col-md-4">
                        <RadzenLabel Text="@(translationState.Translate("CAN_Number") + "*")" Component="CANNumber" />
                        <RadzenTextBox Placeholder="@translationState.Translate("CAN_Number")" @bind-Value="@(advanceSearch.CANNumber)" Name="CANNumber" />
                    </div>

                    <div class="col-md-4">
                        <RadzenLabel Text="@(translationState.Translate("Case_Number") + "*")" Component="CaseNumber" />
                        <RadzenTextBox Placeholder="@translationState.Translate("Case_Number")" @bind-Value="@(advanceSearch.CaseNumber)" Name="Case_Number" />
